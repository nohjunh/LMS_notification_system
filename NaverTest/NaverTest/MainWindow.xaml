<Window x:Class="NaverTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NaverTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1.5*"/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>
        <StackPanel>
            <Button x:Name="NaverLogin" Content="naver Login"
                    Margin="10" Height="30"
                    Click="NaverLogin_Click"/>

        </StackPanel>
        
        <WebBrowser x:Name="NaverWebBrowser"
                    Grid.Row="1" 
                    Margin="10">
        </WebBrowser>
    </Grid>
</Window>
